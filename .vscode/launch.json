  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Launch",
      "program": "${workspaceFolder}/dist/index.js",
      "args": [
        "-c",
        "sample\\Petclinic-tests-nested.postman_collection.json",
        "-e",
        "sample\\Petclinic-tests.postman_environment.json",
        "--drill-admin-url",
        "localhost:8090",
